[user]
	name = Takashi Kanemoto
	email = t-kanemoto@quartet-mail.com
[core]
	excludesfile = ~/.gitignore_global
[color]
	ui = auto
[alias]
	ad = add .
	st = status
	ci = commit -m
	co = checkout
	cb = checkout -b
	cm = checkout master
	br = branch
	ll = log --decorate --oneline
	mg = merge --no-ff
	mt = mergeto
	df = diff --stat
	ls = ls-files
[merge]
	tool = opendiff
