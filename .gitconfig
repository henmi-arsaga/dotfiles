[user]
	name = Takashi Kanemoto
	email = t-kanemoto@quartet-mail.com
[core]
	excludesfile = ~/.gitignore_global
	precomposeunicode = true
	quotepath = false
	autocrlf = input
[color]
	ui = auto
[alias]
	ad = add .
	st = status
	ci = commit -m
	co = checkout
	cb = checkout -b
	cm = checkout master
	br = branch
	log = log --decorate
	ll = log --decorate --oneline
	mg = merge --no-ff
	mt = mergeto
	df = diff --stat
	ls = ls-files
	re = remote
	pl = pull
	ps = push
	rmd = !git rm $(git ls-files --deleted)
[merge]
	tool = opendiff
